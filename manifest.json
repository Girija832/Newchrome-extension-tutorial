{
	"manifest_version" : 2,
	"name": "SMS",
	"description": "Send SMS",
	"version": "1.0",
	"background": {
      "scripts": ["js/option.js"]
    },
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "index.html"
	},
	"options_page":"options.html",
	"permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ]
}